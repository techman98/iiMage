{
  "name": "mern-bp",
  "version": "1.0.0",
  "description": "Mern Boiler Plate",
  "main": "server.js",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "seedFolders": "node scripts/folderSeed.js",
    "seedImages": "node scripts/imageSeed.js",
    "seedUsers": "node scripts/imageSeed.js",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@okta/okta-react": "^1.2.0",
    "@okta/okta-signin-widget": "^2.19.0",
    "@sendgrid/mail": "^6.4.0",
    "@types/react-stripe-elements": "^1.1.10",
    "@types/stripe": "^6.25.15",
    "animejs": "^3.0.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "component-template": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "firebase": "^6.0.2",
    "i": "^0.3.6",
    "if-env": "^1.0.4",
    "mdbreact": "^4.13.0",
    "mongoose": "^5.5.9",
    "morgan": "^1.9.0",
    "npm": "^6.9.0",
    "pure-react-carousel": "^1.20.0",
    "react-anime": "^2.2.0",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-icons": "^3.7.0",
    "react-responsive-carousel": "^3.1.49",
    "react-transition-group": "^4.0.1",
    "reactstrap": "^8.0.0",
    "request": "^2.88.0"
  }
}
